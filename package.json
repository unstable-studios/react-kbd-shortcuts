{
  "name": "react-kbd-shortcuts-monorepo",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel --filter \"react-kbd-shortcuts\" --filter \"react-kbd-shortcuts-sandbox\" run dev",
    "dev:lib": "pnpm --filter react-kbd-shortcuts build:watch",
    "dev:sandbox": "pnpm --filter react-kbd-shortcuts-sandbox dev",
    "build": "pnpm -r build",
    "test": "pnpm --filter react-kbd-shortcuts test",
    "test:watch": "pnpm --filter react-kbd-shortcuts test:watch",
    "test:ui": "pnpm --filter react-kbd-shortcuts test:ui",
    "test:coverage": "pnpm --filter react-kbd-shortcuts test:coverage",
    "lint": "pnpm -r lint",
    "clean": "git clean -xfd && git reset --hard"
  },
  "devDependencies": {
    "tailwindcss": "^4.1.16"
  },
  "version": "0.1.0"
}
